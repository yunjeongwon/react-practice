{
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
    // "airbnb",
    // "airbnb/hooks"
  ],
  "plugins": [
    "import",
    "react-hooks"
  ],
  "rules": {
    "arrow-body-style":"off",
    "react/jsx-filename-extension":"off",
    "react/function-component-definition":"off",
    "linebreak-style":"off",
    "jsx-a11y/label-has-associated-control":"off"
  }
}